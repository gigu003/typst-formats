```{r}
#| output: asis
#| include: !expr as.numeric(cancer_code) %in% c(101:111, 117:122)
#| eval: !expr as.numeric(cancer_code) %in% c(101:111, 117:122)
ii <- title_en
part44 <- paste("Among the ",num1," urban cancer registries, the ASR China incidence rate of ",ii," cancer in males was highest in ",c.i.m[[1]]$des2," followed by ",c.i.m[[2]]$des2,", and ",c.i.m[[3]]$des2,". For females, it was highest in ",c.i.f[[1]]$des2,", followed by ",c.i.f[[2]]$des2,", and ",c.i.f[[3]]$des2,".",sep="")

part55 <- paste("The ASR China mortality rate of ",ii," cancer in males was highest in ",c.m.m[[1]]$des2," followed by ",c.m.m[[2]]$des2,", and ",c.m.m[[3]]$des2,". For females, it was highest in ",c.m.f[[1]]$des2,", followed by ",c.m.f[[2]]$des2,", and ",c.m.f[[3]]$des2,".",sep="")
  
part66 <- paste("Among the ",num2," rural cancer registries, the ASR China incidence rate of ",ii," cancer in males was highest in ",r.i.m[[1]]$des2," followed by ",r.i.m[[2]]$des2,", and ",r.i.m[[3]]$des2,". For females, it was highest in ",r.i.f[[1]]$des2,", followed by ",r.i.f[[2]]$des2,", and ",r.i.f[[3]]$des2,".",sep="")
  
part77 <- paste("The ASR China mortality rate of ",ii," cancer in males was highest in ",r.m.m[[1]]$des2," followed by ",r.m.m[[2]]$des2,", and ",r.m.m[[3]]$des2,". For females, it was highest in ",r.m.f[[1]]$des2,", followed by ",r.m.f[[2]]$des2,", and ",r.m.f[[3]]$des2,".",sep="")

cat(part44, sep = "\n")
cat(part55, sep = "\n")
cat(paste0("(Figure 5-",k,"-b)"))
cat("\n")
cat("\n")
cat(part66, sep = "\n")
cat(part77, sep = "\n")
cat(paste0("(Figure 5-",k,"-c)"))
```


