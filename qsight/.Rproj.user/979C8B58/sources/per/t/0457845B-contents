#| output: asis
#| include: !expr as.numeric(cancer_code) %in% c(101:111, 117:122)
#| eval: !expr as.numeric(cancer_code) %in% c(101:111, 117:122)
area <- site.area |> 
  filter(code == cancer_code) |> 
  mutate(name2 = str_replace(string = name2,
                             pattern = "Areas",
                             replacement = ""))
c.i.m <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city == 1, sex == "男性", type == "incidence") |> 
    group_by(order) |> 
    group_split()

c.i.f <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city==1,sex=="女性",type=="incidence") |> 
    group_by(order) |> 
    group_split()
  
c.m.m <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city==1,sex=="男性",type=="mortality") |> 
    group_by(order) |> 
    group_split()
  
c.m.f <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city==1,sex=="女性",type=="mortality") |> 
    group_by(order) |> 
    group_split()
  
r.i.m <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city==0,sex=="男性",type=="incidence") |> 
    group_by(order) |> 
    group_split()

r.i.f <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city==0,sex=="女性",type=="incidence") |> 
    group_by(order) |> 
    group_split()

r.m.m <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city==0,sex=="男性",type=="mortality") |> 
    group_by(order) |> 
    group_split()

r.m.f <- area |> 
    mutate(des=paste(name,"（",rate,"/10万）",sep=""),
           des2=paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city==0,sex=="女性",type=="mortality") |> 
    group_by(order) |> 
    group_split()
  
part4 <- paste("在",num1,"个城市肿瘤登记地区中，男性",title_cn,"癌标化发病率最高的登记处是",c.i.m[[1]]$des,"，其次是",c.i.m[[2]]$des,"和",c.i.m[[3]]$des,"；女性标化发病率最高的是",c.i.f[[1]]$des,"，其次是",c.i.f[[2]]$des,"和",c.i.f[[3]]$des,"。",sep="")

part5 <- paste("男性标化死亡率最高的登记处是",c.m.m[[1]]$des,"，其次是",c.m.m[[2]]$des,"和",c.m.m[[3]]$des,"；女性标化死亡率最高的是",c.m.f[[1]]$des,"，其次是",c.m.f[[2]]$des,"和",c.m.f[[3]]$des,"。",sep="")

part6 <- paste("在",num2,"个农村肿瘤登记地区中，男性",title_cn,"癌标化发病率最高的登记处是",r.i.m[[1]]$des,"，其次是",r.i.m[[2]]$des,"和",r.i.m[[3]]$des,"；女性标化发病率最高的是",r.i.f[[1]]$des,"，其次是",r.i.f[[2]]$des,"和",r.i.f[[3]]$des,"。",sep="")
    
part7 <- paste("男性标化死亡率最高的登记处是",r.m.m[[1]]$des,"，其次是",r.m.m[[2]]$des,"和",r.m.m[[3]]$des,"；女性标化死亡率最高的是",r.m.f[[1]]$des,"，其次是",r.m.f[[2]]$des,"和",r.m.f[[3]]$des,"。",sep="")
 
cat(part4, sep = "\n")
cat(part5, sep = "\n")
cat(paste0("（ 图5-",k,"-b ）"))
