```{r}
#| tbl-cap: !expr 'paste0("表 5-",k,":  ",year,"年",province,"肿瘤登记地区",title_cn,"癌发病死亡情况")'
#| include: !expr as.numeric(cancer_code) %in% c(101:111,117:122)
## 准备数据
data <- site |> 
  select(-code, -cate, -cate2) |> 
    mutate(name = ifelse(name == "全省", "全省\n(All)",
                       ifelse(name == "城市地区", "城市地区\n(Urban area)",
                              ifelse(name == "农村地区", "农村地区\n(Rural area)",name))),
         sex = ifelse(sex == "合计", "合计(All)",
                      ifelse(sex == "男性", "男性(Male)",
                             ifelse(sex == "女性", "女性(Female)",sex))),
         type = ifelse(type == "发病", "发病(Incidence)", "死亡(Mortality)")) |>
  as_grouped_data(groups = c("type"))
## 设置表格表头
header <- data.frame(
  col_keys = colnames(data),
  first = c(" ","地区\nArea","性别\nGender","病例数\nCases","粗率",
            "构成","中标率","世标率","累积率\nCumulate Rate\n0-74(%)"))

## 制作表格
data <- data |> flextable()
data <- merge_at(data, i = 1, j = 1:8)
#data <- merge_at(data, i = 11, j = 1:8)
data <- set_header_df(data, mapping = header, key = "col_keys")
data <- colformat_num(data, j = 5:9, digits = 5.2) 
data <- data |> 
  compose(j = 5, value = as_paragraph("粗率\nCrude Rate\n(1/10", as_sup("5"), ")"),
          part = "header") |> 
  compose(j = 6, value = as_paragraph("构成\nProportion\n(%)"),
          part = "header") |> 
  compose(j = 7, value = as_paragraph("中标率\nASR China\n(1/10", as_sup("5"), ")"),
          part = "header") |> 
  compose(j = 8, value = as_paragraph("世标率\nASR World\n(1/10", as_sup("5"), ")"),
          part = "header") |> 
  align(align = "center", part = "all") |> 
  bold(bold = TRUE, part = "header") |> 
  width(j = 1, width = 0.3) |>
  width(j = 2, width = 0.8) |> 
  width(j = 3, width = 0.9) |> 
  width(j = c(4:5, 7:8), width = 0.6) |> 
  width(j = c(6), width = 0.8) |> 
  #width(j = c(7:8), width = 0.5) |> 
  align(j = c(2:3), align = "right", part = "all") |> 
  fontsize(size = 9, part = "all") |>
  theme_booktabs()
data
```


```{r}
#| tbl-cap: !expr 'paste0("表 5-",k,":  ",year,"年",province,"肿瘤登记地区",title_cn,"癌发病死亡情况")'
#| include: !expr as.numeric(cancer_code) %in% c(112, 113, 114, 115, 116)
data <- site |>
  select(-code,-sex,-cate,-cate2) |> 
  mutate(name = ifelse(name == "全省", "全省\n(All)",
                       ifelse(name == "城市地区", "城市地区\n(Urban area)",
                              ifelse(name == "农村地区", "农村地区\n(Rural area)",name))),
         type = ifelse(type == "发病", "发病(Incidence)", "死亡(Mortality)")) |> 
  as_grouped_data(groups = c("type"))
header <- data.frame(
  col_keys = colnames(data),
  first=c(" ", "地区\nArea", "病例数\nCases", "粗率",
          "构成", "中标率", "世标率", "累积率\nCumulate Rate\n0-74(%)"))

data |> 
  flextable() |> 
  set_header_df(mapping = header, key = "col_keys" ) |> 
  colformat_num(j = 4:8, digits = 5.2) |> 
  compose(j = 4, value = as_paragraph("粗率\nCrude Rate\n(1/10", as_sup("5"),")"),
          part="header") |> 
  compose(j = 5, value = as_paragraph("构成\nProportion\n(%)"),
          part="header") |> 
  compose(j = 6, value = as_paragraph("中标率\nASR China\n(1/10", as_sup("5"),")"),
          part="header") |> 
  compose(j = 7, value = as_paragraph("世标率\nASR World\n(1/10", as_sup("5"),")"),
          part="header") |> 
  merge_at(i = 1, j = c(1:8)) |> 
  merge_at(i = 5, j = c(1:8)) |> 
  align(align = "center", part="all") |> 
  align(j = 1, align="right", part="body") |> 
  fontsize(size = 10, part = "all") |> 
  bold(bold = TRUE, part = "header") |> 
  bold(j = 1, i = ~ !is.na(type), bold = TRUE, part = "body" ) |> 
  width(j = c(1), width = 0.3) |> 
  width(j = c(2), width = 1.2) |> 
  width(j = c(3), width = 0.6) |> 
  width(j = c(4, 5, 6, 7), width = 0.9) |> 
  theme_booktabs()
```


