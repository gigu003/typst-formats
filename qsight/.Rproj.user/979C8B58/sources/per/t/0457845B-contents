
```{r}
site_cn <- c("口腔","鼻咽","食管","胃","结直肠","肝","胆囊","胰腺","喉","肺",
          "骨","女性乳腺","子宫颈","子宫体","卵巢","前列腺","肾及泌尿系统不明",
          "膀胱","脑","甲状腺","淋巴瘤","白血病")
site_en <- c("oral cavity & pharynx", "nasopharynx", "esophagus", "stomach",
             "colon rectum", "liver", "gallbladder", "pancreas", "larynx",
             "lung", "bone", "female breast", "cervix", "uterus", "ovary",
             "prostate", "kidney & unspecified urinary organs", "bladder",
             "brain", "thyroid", "lymphoma", "leukemia")
title_cn <- site_cn[as.numeric(cancer_code) - 100]
title_en <- site_en[as.numeric(cancer_code) - 100]
title_ens <- str_to_title(title_en)
k <- as.numeric(cancer_code) - 100
```

## `r title_cn`

```{r}
#| output: asis
#| include: !expr as.numeric(cancer_code) %in% c(101:111, 117:122)
sex_ratio1 <- round(site[2,6] / site[3,6], 2)
sex_ratio2 <- round(site[11,6] / site[12,6], 2)
area_ratio1 <- round(site[4,6] / site[7,6], 2)
area_ratio2 <- round(site[13,6] / site[16,6], 2)
des1 <- gen_des1(type = "中标发病率", sex_ratio1, area_ratio1, cancer_code, lang = "zh-cn")
des2 <- gen_des1(type = "中标死亡率", sex_ratio2, area_ratio2, cancer_code, lang = "zh-cn")

part1 <- paste(year, "年，",province,"肿瘤登记地区",title_cn,"癌的发病率为",site[1,4],"/10万，中标率为",site[1,6],"/10万，世标率为",site[1,7],"/10万，发病例数占全部癌症新发病例数的",site[1,5],"%；","其中，男性和女性发病率分别为",site[2,4],"/10万和",site[3,4],"/10万，城市地区和农村地区发病率分别为",site[4,4],"/10万和",site[7,4],"/10万；",des1, sep="")

part2 <- paste("同期，",title_cn,"癌的死亡率为",site[10,4],"/10万，中标率为",site[10,6],"/10万，世标率为",site[10,7],"/10万，死亡病例数占全部癌症死亡病例数的",site[10,5],"%；","其中，男性和女性死亡率分别为",site[11,4],"/10万和",site[12,4],"/10万，城市地区和农村地区死亡率分别为",site[13,4],"/10万和",site[16,4],"/10万；",des2,sep="")

cat(part1, sep = '\n')
cat(part2)
cat(paste0("（见表5-", k, "）"))
