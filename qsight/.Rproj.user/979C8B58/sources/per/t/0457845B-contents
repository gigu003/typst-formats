#| output: asis
#| include: !expr as.numeric(cancer_code) %in% c(112:116)
#| eval: !expr as.numeric(cancer_code) %in% c(112:116)
area <- site.area |> 
  filter(code == as.integer(cancer_code), sex == "合计") |> 
    mutate(name2 = str_replace(string = name2,
                               pattern = "Areas",
                               replacement = ""))
c.i <- area |> 
    mutate(des = paste(name,"（",rate,"/10万）", sep=""),
           des2 = paste(name2," (",rate," per 100 000)", sep="")) |> 
    filter(city == 1, type=="incidence") |> 
    group_by(order) |> 
    group_split()
  
c.m <- area |> 
    mutate(des = paste(name,"（",rate,"/10万）", sep=""),
           des2 = paste(name2," (",rate," per 100 000)", sep="")) |> 
    filter(city == 1, type == "mortality") |> 
    group_by(order) |> 
    group_split()
  
r.i <- area |> 
    mutate(des = paste(name,"（",rate,"/10万）",sep=""),
           des2 = paste(name2," (",rate," per 100 000)",sep="")) |> 
    filter(city == 0, type == "incidence") |> 
    group_by(order) |> 
    group_split()
  
r.m <- area |> 
    mutate(des = paste(name,"（",rate,"/10万）", sep=""),
           des2 = paste(name2," (",rate," per 100 000)", sep="")) |> 
    filter(city == 0, type == "mortality") |> 
    group_by(order) |> 
    group_split()

part4 <- paste("在",num1,"个城市肿瘤登记地区中，",title_cn,"癌标化发病率最高的登记处是",c.i[[1]]$des,"，其次是",c.i[[2]]$des,"和",c.i[[3]]$des,"；", sep="")

part5 <- paste("标化死亡率最高的登记处是",c.m[[1]]$des,"，其次是",c.m[[2]]$des,"和",c.m[[3]]$des,"。",sep="")

