#| output: asis
k <- cancer_code - 100
inc.max<-fb.age.max[fb.age.max$code==cancer_code,]$age
mor.max<-sw.age.max[sw.age.max$code==cancer_code,]$age
part3 <- paste(title_cn,"癌年龄别发病率在0-",fb.low[k],"岁年龄段处于较低水平，",fb.start[k],"岁以后快速上升，在",inc.max,"岁达到峰值；","年龄别死亡率在0-",sw.low[k],"岁年龄段处于较低水平，",sw.start[k],"岁以后快速上升，在",mor.max,"岁达到峰值；",ifelse(cancer_code %in% c(112,113,114,115,116),"",paste0("年龄别发病率和死亡率均呈现",ifelse(cancer_code %in% c(120),"女性高于男性","男性高于女性"),"的特点；")),"城市地区和农村地区年龄别率虽然有一定差异，但是总体趋势类同。",sep="")
cat(part3, sep = '\n')
cat(paste0("（ 图5-",k,"-a ）"))
