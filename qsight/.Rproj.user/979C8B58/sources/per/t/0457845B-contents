des2 <- gen_des1(type = "中标死亡率", sex_ratio2, area_ratio2, cancer_code, lang = "zh-cn")

part1 <- paste(year,"年，",province,"肿瘤登记地区",title_cn,"癌的发病率为",site[1,4],"/10万，中标率为",site[1,6],"/10万，世标率为",site[1,7],"/10万，发病例数占全部癌症新发病例数的",site[1,5],"%；","其中，城市地区和农村地区发病率分别为",site[2,4],"/10万和",site[3,4],"/10万；",des1,sep="")

